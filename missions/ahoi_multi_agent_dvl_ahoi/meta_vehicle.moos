ServerHost = localhost
ServerPort = $(MOOS_PORT)
Community  = $(VNAME)

#include plugs/shared/plug_origin_warp.moos

//------------------------------------------------------------
Processconfig = ANTLER
{
  MSBetweenLaunches = 100
   
  Run = MOOSDB             @ NewConsole = false
  Run = pLogger            @ NewConsole = false
  Run = pContactMgrV20     @ NewConsole = false
  Run = uProcessWatch      @ NewConsole = false
  Run = pShare             @ NewConsole = false
  Run = pNodeReporter      @ NewConsole = false
  Run = pHostInfo          @ NewConsole = false
  Run = uFldNodeBroker     @ NewConsole = false
  Run = uFldMessageHandler @ NewConsole = false
  Run = uLoadWatch         @ NewConsole = false
  Run = uMemWatch          @ NewConsole = false
  Run = pRealm             @ NewConsole = false
  Run = pHelmIvP           @ NewConsole = false
  Run = pMarinePID         @ NewConsole = false

  ## runs but quits directly
  
  #Run = ../../src/pAhoiModemManager/pymoos_app_test.py  @ NewConsole = true, ExtraProcessParams=PythonParams
  #Run = ../../src/pAhoiModemManager/pyAhoi_Anchor_Manager.py  @ NewConsole = true, ExtraProcessParams=PythonParams
  
  #PythonParams = localhost,9000,alder
  
  PythonParams = localhost,$(MOOS_PORT),$(VNAME)

  #ifdef AHOI_TYPE MOBILE
    Run = ../../src/pAhoiModemManager/pyAhoi_MobileBase_Manager.py  @ NewConsole = true, ExtraProcessParams=PythonParams
    PythonParams = ServerHost,$(MOOS_PORT),$(VNAME)
  #elseifdef AHOI_TYPE ANCHOR
    Run = ../../src/pAhoiModemManager/pyAhoi_Anchor_Manager.py  @ NewConsole = true, ExtraProcessParams=PythonParams
    PythonParams = ServerHost,$(MOOS_PORT),$(VNAME)
  #elseifdef AHOI_TYPE TEST
    #Run = ../../src/pAhoiModemManager/pymoos_app_test.py  @ NewConsole = true, ExtraProcessParams=PythonParams
    Run = ../../src/pAhoiModemManager/pymoos_app_base_template.py  @ NewConsole = true, ExtraProcessParams=PythonParams
    PythonParams = ServerHost,$(MOOS_PORT),$(VNAME)
  #endif
  

#ifdef XMODE SIM
  Run = uSimMarineV22      @ NewConsole = false
#else
  Run = iM300              @ NewConsole = false
  Run = iM300Health        @ NewConsole = false

  #ifdef ROLL MNODE
    Run = iTracker650         @ NewConsole = false
  #endif 
#endif
}
	
